{
    "name": "inanepain/dumper",
    "description": "A little tool to help with debugging by writing a `var_dump` like message unobtrusively to a collapsible panel at the bottom of a page.",
    "type": "library",
    "license": "UNLICENSE",
    "homepage": "http://vaieen.local:3000/inane/dumper",
    "readme": "README.md",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "peep@inane.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "peep@inane.co.za",
        "issues": "http://vaieen.local:3000/iane"
    },
    "keywords": [
        "inane",
        "dumper",
        "debug"
    ],
    "autoload": {
        "psr-4": {
            "Inane\\Dumper\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "discard-changes": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "inanepain/stdlib": ">=0.1.6 || dev-master || dev-develop"
    },
    "require-dev": {}
}
