{
	"name" : "inanepain/dumper",
	"description" : "A little tool to help with debugging by writing a `var_dump` like message unobtrusively to a collapsable panel at the bottom of a page.",
	"type" : "project",
	"license" : "UNLICENSE",
	"authors" : [{
			"name" : "Philip Michael Raab",
			"email" : "peep@inane.co.za",
			"role" : "Developer"
		}
	],
	"homepage" : "http://inane.co.za",
	"readme": "README.md",
	"support" : {
		"email" : "peep@inane.co.za",
		"issues" : "http://inane.co.za"
	},
	"keywords" : [
		"inane",
		"debug"
	],
	"autoload" : {
		"psr-4" : {
			"Inane\\Dumper\\" : "src/"
		}
	},
	"config": {
        "preferred-install" : "dist",
		"optimize-autoloader" : true,
		"discard-changes" : true,
		"sort-packages": true
    },
	"require" : {
		"php" : ">=8.1",
		"inanepain/stdlib": ">=0.1.0 || dev-develop || dev-master"
	},
	"require-dev": {
    }
}
